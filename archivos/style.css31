    margin: 0px;
}

input.ra {
    float: left;
    height: 6px;
}
#footer .social {
	width:100%;
    background-color: #00A79D;
	color: white;
	padding: 8px 20px;
}
#footer .social a {
	color:white;
}
section#footer h1.logo {
    margin-bottom: 0;
    text-align: right;
    overflow: hidden;
}
section#header h1.logo {
    /*margin-bottom: 0;*/
    text-align: right;
    overflow: hidden;
    height: 104px;
	margin-bottom:10px;
}

#contacto form .control {
    position: relative;
    height: 45px;
    margin-bottom: 0px;
}
#contacto form .control:hover label.error {
    height: 0;
}
#contacto form label.error {
    position: absolute;
    bottom: -8px;
    background-color: rgba(240, 65, 36, 0.56);
    color: white;
    text-shadow: 0 0 5px rgba(0,0,0,.3);
    width: 100%;
    border-radius: 0 0 3px 3px;
    font-size: 10px;
    line-height: 15px;
    font-style: italic;
    padding-left: 9px;
    height: 15px;
    transition: height .2s ease-in-out;
    -webkit-transition: height .2s ease-in-out;
    -moz-transition: height .2s ease-in-out;
    -o-transition: height .2s ease-in-out;
    overflow: hidden;
}
#contacto form input[type="submit"]:disabled {
    background-color: #ADC2CF;
}
#gracias, #gracias-mensaje {
    text-align: center;
}
#gracias h1, #gracias h4, #gracias-mensaje h1, #gracias-mensaje h2 {
