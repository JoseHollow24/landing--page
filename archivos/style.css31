    margin-bottom: 10px;
    color: white;
    font-weight: lighter;
}
#gracias .logo-firma {
	margin-top: 30px;
}

/* Smartphones (portrait and landscape) ----------- */
@media only screen
and (min-device-width : 320px)
and (max-device-width : 480px) {
    #header {
        max-height: inherit;
    }
    #header h1 ,#header h2 {
        text-align: center;
    }
	section#header h1 {
		margin-top:0px;
		margin-bottom: 16px;
	}
	section#header h1.logo {
    	text-align: center;
	}
    section#mensaje.a, section#mensaje.b {
        background-position: 50% 0;
        background-size: 100%;
    }
    #puntos ul {
        /*background-color: white;
        box-shadow: 0 -3px 3px rgba(0,0,0,.2);
        margin-top: 350px;*/
        padding: 20px;
		position:relative;
		margin-top: -150px;
    }
    #puntos ul li {
        font-size: 14px;
        line-height: 1em;
        background-position: 0;
        margin-bottom: 12px;
    }
	.mujer {
		position:relative;
		top:-20px;
	}
	.promo {
		top:0px;
		text-align:center;margin-bottom: 482px;
	}
	
	.large-10.medium-9.small-12.columns.texp p {
    line-height: 16px;
    /* padding-top: 5px; */
    font-size: 15px!important;
    font-weight: normal!important;
    text-align: justify;
    width: 97%;
    margin-top: 12px;
}
	
	.large-8.medium-8.small-12.columns.medios {
    width: 322px;
    margin: auto;
    float: none;
    padding-top: 35px;
}
	#doctor .left img {
		margin-top: 0px;
	}
	#foto, #doctor h4 {
		text-align:center;
	}
	
	#face {
		padding: 0px;
	}
	
	.pareja {
    background: url(../pareja.jpg) right top 119px no-repeat;
}
}


