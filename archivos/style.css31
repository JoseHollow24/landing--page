#contacto {
    padding: 20px;
    height: 550px;
}
#contacto .small {
    color: white;
    line-height: 12px;
    font-size: 12px !important;
}
#contacto h3 {
    font-size: 24px;
    text-align: center;
    margin-top: 20px;
    color: white;
    font-family:"Arial Black", Gadget, sans-serif;
    padding: 0px 10px;
	font-weight:900;
}
#contacto form input, #contacto form select {
    border: 0;
    box-shadow: none;
    height: 45px;
    border-radius: 3px;
    font-size: 14px;
}
#contacto .control {
    margin: 20px 0;
}
#contacto .control.submit {
    text-align: center;
}
#contacto form label {
    display: none;
}
#contacto form input {
   color: gray;
    font-weight: 600;
    background: #3f2a5e;
    border-radius: 0px;
}
#contacto form select {
    color: #666;
}
#contacto form input[type="submit"] {
    background-color: #FEF200;
    color: #00a79d;
    font-family:"Arial Black", Gadget, sans-serif;
    text-transform: uppercase;
    cursor: pointer;
    transition: background-color .3s;
    box-shadow: 0 0 3px rgba(255,255,255,.1),
    inset 2px 2px 2px rgba(255,255,255,.15),
    inset -2px -2px 2px rgba(0,0,0,.15);
    font-size: 14px;
    width: 90%;
    margin: 0 auto;
    -webkit-appearance: none;
}
#contacto form input[type="submit"]:hover {
    /*background-color: #67a345;
    box-shadow: 0 0 3px rgba(255,255,255,.1),
    inset -1px -1px 1px rgba(255,255,255,.15),
    inset 1px 1px 1px rgba(0,0,0,.15);*/
}
